<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="authorization.client_id" />
    </parameters>
    <services>
        <!-- LogService -->
        <service id="authorization.service" class="Mixasmix\AuthBundle\\Service\AuthService">
            <argument key="$authClientId" type="string">%authorization.client_id%</argument>
        </service>

        <service id="Mixasmix\AuthBundle\\Service\AuthService" alias="authorization.service" />
        <!-- End LogService -->
    </services>
</container>