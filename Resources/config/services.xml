<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="auth.url" />
    </parameters>
    <services>
        <!-- LogService -->
        <service id="auth.service" class="Mixasmix\AuthBundle\Service\AuthService">
            <argument key="$authProdavayApiUrl" type="string">%auth.url%</argument>
        </service>

        <service id="Mixasmix\AuthBundle\Service\AuthService" alias="auth.service" />
        <!-- End LogService -->
    </services>
</container>